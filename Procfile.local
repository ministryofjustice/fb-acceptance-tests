# The name of the application as shown on the console (default is 'Procodile')
app_name: Acceptance tests

env:
  SUBMITTER_URL: 'http://submitter-app:3000'
  USER_DATASTORE_URL: 'http://datastore-app:3000'
  USER_FILESTORE_URL: 'http://filestore-app:3000'
  SERVICE_SECRET: secret
  SERVICE_TOKEN: token
  SERVICE_SLUG: slug
  ENCODED_PRIVATE_KEY: ''
  ENCODED_PUBLIC_KEY: ''

# Per-process configuration options
processes:
  features-email-app:
    quantity: 1
    respawn_window: 3
    env:
      PORT: 16080
      SERVICE_PATH: ./forms/features/email-output
      SERVICE_OUTPUT_CSV: 'true'
      SERVICE_OUTPUT_EMAIL: 'form-builder-developers@digital.justice.gov.uk,form-builder-team@digital.justice.gov.uk'
  features-json-app:
    quantity: 1
    respawn_window: 3
    env:
      SERVICE_PATH: ./forms/features/json-output
      SERVICE_OUTPUT_JSON_ENDPOINT: http://base-adapter-app:4567/submission
      SERVICE_OUTPUT_JSON_KEY: 19a56ee0e50dd83g
      PORT: 16081
  features-maintenance-mode-app:
    quantity: 1
    respawn_window: 3
    env:
      SERVICE_PATH: ./forms/features/email-output
      MAINTENANCE_MODE: 'true'
      PORT: 16082
  features-conditional-steps-app:
    quantity: 1
    respawn_window: 3
    env:
      SERVICE_PATH: ./forms/features/conditional-steps
      PORT: 16083
  features-save-and-return:
    quantity: 1
    respawn_window: 3
    env:
      SERVICE_PATH: ./forms/features/save-and-return-module
      SUBMITTER_URL: 'http://save-and-return-simulator:3000'
      PORT: 16084
  components-autocomplete-app:
    quantity: 1
    respawn_window: 3
    env:
      SERVICE_PATH: ./forms/components/autocomplete
      PORT: 18000
